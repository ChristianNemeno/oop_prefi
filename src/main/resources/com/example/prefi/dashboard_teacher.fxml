<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="grading_system.dashboard_teacher">
    <children>
        <TableView fx:id="tblView" layoutX="125.0" layoutY="100.0" prefHeight="200.0" prefWidth="350.0">
            <columns>
                <TableColumn fx:id="coursecode_col" prefWidth="119.0" text="Course code" />
                <TableColumn fx:id="coursename_col" prefWidth="128.0" text="Course name" />
                <TableColumn fx:id="grade_col" prefWidth="102.0" text="Grade" />
            </columns>
        </TableView>
        <Button fx:id="logoutBtn" layoutX="511.0" layoutY="41.0" mnemonicParsing="false" onAction="#onLogoutClicked" text="Logout" />
        <Label layoutX="150.0" layoutY="66.0" text="Name:" />
        <TextField fx:id="tfName" layoutX="200.0" layoutY="62.0" prefHeight="25.0" prefWidth="89.0" />

    </children>
</AnchorPane>
